<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
       
  <bean id="bacc" class="com.srm.bank.BankAcc">
  <property name="accno" value="123"/>
  <property name="bcust" ref="bcustomer"/>
  </bean>
  
   <bean id="bcustomer" class="com.srm.bank.BankCustomer">
  <property name="custname"  value="Sumathy"/>
  </bean>
  
   <bean id="bacc1" class="com.srm.bank.BankAcc">
  <property name="accno" value="321"/>
  <property name="bcust" ref="bcustom"/>
  </bean>
  
  <bean id="bcustom" class="com.srm.bank.BankCustomer">
  <property name="custname"  value="Mathew"/>
  </bean>
   </beans>
  
